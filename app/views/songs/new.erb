<form action = "/songs" method = "POST">
<label>Name:</label><input type="text" name="song[name]" id="Name"><br>
<label>Artist:</label><input type="text" name="song[artist]" id="Artist Name"><br>
<label>Genres:</label>
<%@genres.each do |genre|%>
<input  id="<%=genre.name%>" type="checkbox"name="song[genres][]"value="<%= genre.name %>"><%= genre.name %><br>
<%end%>
<input type = "submit" name = "Create">
</form>